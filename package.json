{
	"name": "www-starter",
	"description": "A Next.js website starter for XORS projects.",
	"version": "1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"turbo": "next dev --turbopack",
		"build": "next build",
		"build-icons": "svgr --out-dir app/components/icons/generated --icon --replace-attr-values '#fff=currentColor' --typescript icon-svg ",
		"clean": "rm -rf node_modules",
		"type-check": "tsc --pretty --noEmit",
		"test": "vitest run --pool=forks",
		"test:watch": "vitest --pool=forks",
		"start": "next start",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write --unsafe .",
		"format": "biome format .",
		"format:fix": "biome format --write ."
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/react-query": "^5.74.4",
		"@types/lodash": "4.14.202",
		"@types/node": "20.10.6",
		"@types/react": "18.2.46",
		"@types/react-dom": "18.2.18",
		"autoprefixer": "10.4.16",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lodash": "^4.17.21",
		"next": "15.3.1",
		"postcss": "8.4.33",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-use": "^17.4.2",
		"sonner": "^2.0.5",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "3.4.1",
		"typescript": "5.7.3",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@svgr/cli": "^8.1.0",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.46",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"jsdom": "^23.1.0",
		"postcss": "^8.4.33",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vitest": "^1.1.3"
	}
}
