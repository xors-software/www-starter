{
	"name": "www-starter",
	"description": "A Next.js website starter for XORS projects.",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"turbo": "next dev --turbopack",
		"build": "next build",
		"build-icons": "svgr --out-dir app/components/icons/generated --icon --replace-attr-values '#fff=currentColor' --typescript icon-svg",
		"clean": "rm -rf node_modules",
		"type-check": "tsc --pretty --noEmit",
		"test": "vitest run --pool=forks",
		"test:watch": "vitest --pool=forks",
		"start": "next start",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write --unsafe .",
		"format": "biome format .",
		"format:fix": "biome format --write ."
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-slot": "^1.2.4",
		"@tanstack/react-query": "^5.90.12",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lodash": "^4.17.21",
		"next": "^16.0.8",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-use": "^17.6.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@svgr/cli": "^8.1.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.21",
		"@types/node": "^22.19.2",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^4.7.0",
		"autoprefixer": "^10.4.22",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	}
}
